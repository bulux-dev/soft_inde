{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "soft": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/soft",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "src/styles.css",
              "public/assets/css/bootstrap.css",
              "public/assets/css/bootstrap-datetimepicker.min.css",
              "public/assets/css/animate.css",
              "public/assets/plugins/select2/css/select2.min.css",
              "public/assets/css/dataTables.bootstrap4.min.css",
              "public/assets/plugins/fontawesome/css/fontawesome.min.css",
              "public/assets/plugins/toastr/toatr.css",
              "public/assets/plugins/lightbox/glightbox.min.css",
              "public/assets/plugins/owlcarousel/owl.carousel.min.css",
              "public/assets/plugins/owlcarousel/owl.theme.default.min.css",
              "public/assets/css/style.css",
              "public/assets/css/driver.css"
            ],
            "scripts": [],
            "allowedCommonJsDependencies": [
              "moment",
              "apexcharts"
            ]
          },
          "configurations": {
            "kairos": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "25mb",
                  "maximumError": "25mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.kairos.ts"
                }
              ],
              "outputHashing": "all"
            },
            "kairosdev": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "25mb",
                  "maximumError": "25mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.kairosdev.ts"
                }
              ],
              "outputHashing": "all"
            },
            "kairosprod": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "25mb",
                  "maximumError": "25mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.kairosprod.ts"
                }
              ],
              "outputHashing": "all"
            }
          },
          "defaultConfiguration": "kairos"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "kairos": {
              "buildTarget": "soft:build:kairosdev"
            },
            "kairosprod": {
              "buildTarget": "soft:build:kairosprod"
            }
          },
          "defaultConfiguration": "kairos"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}